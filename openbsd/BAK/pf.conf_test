# more pf.conf                                                                                                                        

xo = "bge0"
internal = "em0"
local = "lo0"

block in
pass out

nat on $xo -> ($xo)
pass in on $internal from 192.168.10.0/24 to any

pass in on $xo proto icmp to any 
#pass in on $xo proto tcp to any port 22
pass in on $xo proto tcp from 67.95.201.192/27 to any port 22


