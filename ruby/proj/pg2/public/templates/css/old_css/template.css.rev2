div {
	border:0 solid red;
}

body {
	font-family:Arial,Helvetica,sans-serif;
	font-size:12px;
	margin:0;
	padding:0;
}

img {
	border:medium none;
}

.clear {
	clear:both;
	line-height:0;
	margin:0;
	padding:0;
}


#container {
	margin:0 auto;
	width:965px;
}

#theater {
	margin:0 auto;
	position:relative;
	top:-7px;
}

#search {
}

#ticker {
	float: left;
	height: 20px;
	padding-top: 2px;
	width: 100%;
}

a.Gloss {
	border-bottom:1px double;
	line-height:1.7em;
}

h1 {
	color:#423132;
	font-weight:bold;
	margin-left:107px;
	text-transform:uppercase;
}

