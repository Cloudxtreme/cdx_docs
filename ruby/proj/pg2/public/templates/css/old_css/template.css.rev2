#header {
	height:109px;
	overflow:hidden;
}

#logo {
	float:left;
	height:87px;
	padding-left:27px;
	width:130px;
}

#logo a {
	outline:medium none;
}

#banner {
	position:relative;
}

#navigation {
	background:url(../images/nav_bg.jpg) no-repeat scroll right top transparent;
	float:right;
	height:45px;
	margin-right:3px;
	width:800px;
}

#header-right {
	float:right;
}

#buckets {
	margin:0 auto;
	min-height:120px;
	width:inherit;
}

#bkt1, #bkt2, #bkt3 {
	float:left;
	min-height:120px;
	padding-bottom:10px;
	padding-left:2px;
	width:33%;
}

#bkt1 img, #bkt2 img, #bkt3 img {
	border:medium none;
	margin-left:0;
	padding-bottom:18px;
}

#bkt1 p, #bkt2 p, #bkt3 p {
	color:#423132;
	line-height:16px;
	margin:0;
	padding-bottom:13px;
	padding-left:3px;
	padding-top:0;
}

#bkt1 a, #bkt2 a, #bkt3 a {
	border:medium none;
	outline:medium none;
}

#bkt3 img {
	padding-bottom:21px;
}

#hmain {
	float:left;
	padding-bottom:20px;
	padding-left:22px;
	width:758px;
}

#hmain #content p {
	color:#423132;
	line-height:18px;
	margin-left:0;
	padding-left:0;
}

#hmain #minheight {
	float:right;
	height:100px;
	width:1px;
}

#hmain #content {
	color:#423132;
	float:left;
	margin-top:5px;
	padding-left:2px;
	padding-right:28px;
	width:565px;
}

#hmain #content_right {
	float:right;
	margin-top:5px;
	padding-top:27px;
	width:158px;
}

h2 {
	padding-bottom:5px;
	padding-left:2px;
}

#footer {
	background:url(../images/footer_line.gif) no-repeat scroll left top transparent;
	color:#C42032;
	margin:0 auto;
	padding-top:17px;
	width:959px;
}

#footer p {
	color:#C42032;
}

#footer #copyright {
	float:left;
	margin-top:0;
	padding-top:0;
}

#footer #copyright p {
	color:#423132;
	margin-top:0;
	padding-top:0;
}

#footer #footer_nav {
	float:right;
	margin-top:0;
	padding-top:0;
}

#footer #footer_nav p {
	margin-top:0;
	padding-top:0;
}

#footer #footer_nav a {
	margin-top:0;
	padding-left:3px;
	padding-right:3px;
	padding-top:0;
	text-decoration:none;
}

#footer #footer_nav a:hover {
	color:#C42032;
}

#noNav {
	float:left;
	margin-bottom:50px;
	margin-left:0;
	padding-left:0;
	width:182px;
}

#noNav #subnav_main {
	background:url(../images/subnav_top.jpg) no-repeat scroll left top#D9E0D8;
	height:180px;
	padding-top:24px;
	width:inherit;
}

#noNav #subnav_bot {
	background:url(../images/subnav_bot.jpg) no-repeat scroll left bottom #D9E0D8;
	height:30px;
	width:inherit;
}

#form_cc {
	float:left;
	margin-bottom:50px;
	margin-left:0;
	padding-left:0;
	width:182px;
}

#form_cc #subnav_main {
	background:url(../images/subnav_top_red.jpg) no-repeat scroll left top #C11334;
	height:180px;
	padding-top:12px;
	width:inherit;
}


