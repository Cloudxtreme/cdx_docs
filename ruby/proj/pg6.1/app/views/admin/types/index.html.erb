<h3>Listing admin_types</h3>

<table>
  <tr>
    <th>Name</th>
    <th>Created by</th>
    <th>Modified by</th>
    <th>Created at</th>
    <th>Modified at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_types.each do |admin_type| %>
  <tr>
    <td><%= admin_type.name %></td>
    <td><%= admin_type.created_by %></td>
    <td><%= admin_type.modified_by %></td>
    <td><%= admin_type.created_at %></td>
    <td><%= admin_type.modified_at %></td>
    <td><%= link_to 'Show', admin_type %></td>
    <td><%= link_to 'Edit', edit_admin_type_path(admin_type) %></td>
    <td><%= link_to 'Destroy', admin_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Type', new_admin_type_path %>
