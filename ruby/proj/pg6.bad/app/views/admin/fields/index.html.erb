<h3>Listing admin_fields</h3>

<table>
  <tr>
    <th>Name</th>
    <th>Label</th>
    <th>Data type</th>
    <th>Created by</th>
    <th>Modified by</th>
    <th>Created at</th>
    <th>Modified at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_fields.each do |admin_field| %>
  <tr>
    <td><%= admin_field.name %></td>
    <td><%= admin_field.label %></td>
    <td><%= admin_field.data_type %></td>
    <td><%= admin_field.created_by %></td>
    <td><%= admin_field.modified_by %></td>
    <td><%= admin_field.created_at %></td>
    <td><%= admin_field.modified_at %></td>
    <td><%= link_to 'Show', admin_field %></td>
    <td><%= link_to 'Edit', edit_admin_field_path(admin_field) %></td>
    <td><%= link_to 'Destroy', admin_field, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Field', new_admin_field_path %>
