
<script type="text/javascript">
$(document).ready( function () {
    $('#example').dataTable();
} );
</script>

<table cellpadding="0" cellspacing="0" border="0" class="display" id="example">
  <thead>
    <tr>
      <% @column_names.each do |column_name| %>
        <th width="20%"><%=column_name.titleize%></th>
      <% end %>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @objects.each do |object| %>
      <tr>
        <% @column_names.each do |column_name| %>
          <td>
						<%= find_value(@parents, column_name, object[column_name])%>
					</td>
        <% end %>
        <td>
          <%= link_to 'Show', object %>/<%= link_to 'Edit', "#{@url}/#{object[:id]}/edit?collection_id=1" %>/<%= link_to 'Destroy', object, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>

  </tbody>
</table><br>
<br>

<%= link_to 'New Field', "#{@url}/new/" %>


